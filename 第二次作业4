#include<stdio.h>
int main(){
	int i=0;
	int j=0;
	printf("input a number");
	scanf("%d",&j);
	for(i=2;i<j;i++){
		if(j%i==0)
		{
			printf("%d isn't prime number\n",j);
			break;
		}
	}
	if(j==i)
	{
		printf("%d is prime number",j);
	}
	if(j==1)
	{
		printf("%d is prime number",j);
	}
	return 0;
}
